{
  "name": "klm-inc",
  "displayName": "KLM inc",
  "description": "Normalize index.html by injecting required snippets and migrating popular libraries to safe CDNJS links.",
  "version": "0.0.3",
  "publisher": "boostclicks",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leontev-E/klm-inc.git"
  },
  "icon": "assets/iconnew.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "html",
    "cdnjs",
    "security",
    "cleanup",
    "index-html",
    "libraries"
  ],
  "activationEvents": [
    "onCommand:klmInc.processIndexHtml"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "klmInc.processIndexHtml",
        "title": "KLM inc: Process index.html/index.php"
      }
    ],
    "keybindings": [
      {
        "command": "klmInc.processIndexHtml",
        "key": "ctrl+alt+shift+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "echo \"No lint configured\""
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1"
  }
}
